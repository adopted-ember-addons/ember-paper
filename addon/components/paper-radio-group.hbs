{{yield (hash
  label=(component this.labelComponent
    setAriaLabelledby=(fn (mut this.ariaLabelledby))
  )
  radio=(component this.radioComponent
    toggle=this.toggle
    disabled=this.disabled
    groupValue=@groupValue
    onChange=(paper-optional-action this.onChange)
    parentComponent=this
    shouldRegister=true)
)}}
